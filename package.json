  "keywords": [
    "pineapple",
    "api",
    "restful",
    "mongodb",
    "mongoose",
    "restify"
  ],
  "preferGlobal" : true,
  "author": "Jospeh Werle <joseph.werle@gmail.com>",
   "bin" : {
    "pineapple" : "./bin/pineapple.js"
  },
  "dependencies": {
    "restify"       : ">= 1.4.4",
